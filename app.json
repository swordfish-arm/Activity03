      "package": "com.alan.martinez.martinezTodo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "382e22e1-8b81-4c9f-aa6c-0d773b8ae390"
